<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tibaes.guardaamigoparte6.ui.ListaAmigosActivity">


    <fragment
        android:id="@+id/fragment_lista_amigos"
        android:name="com.tibaes.guardaamigoparte6.fragment.ListaAmigosFragment"
        android:layout_width="300dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <fragment
        android:id="@+id/fragment_cadastra_amigo_large"
        android:name="com.tibaes.guardaamigoparte6.fragment.CadastraAmigoFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fragment_lista_amigos"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
